{
	"extends": "@wioenena.q/tsconfig",
	"include": [
		"src/**/*.ts",
		"test/**/*.spec.ts",
		"test/**/*.ts"
	],
	"exclude": [
		"node_modules",
		"**/*.d.ts",
	],
	"compilerOptions": {
		"outDir": "dist",
		"declaration": true,
		"module": "CommonJS",
		"target": "ESNext",
		"declarationDir": "dist/types",
		"removeComments": false
	}
}